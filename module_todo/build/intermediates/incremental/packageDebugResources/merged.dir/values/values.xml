    <style name="todo_TranslucentStyle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item> <!-- 背景色透明 -->
        <item name="android:windowIsTranslucent">true</item> <!-- 是否有透明属性 -->
        <item name="android:backgroundDimEnabled">false</item> <!-- 背景是否半透明 -->
        <item name="android:statusBarColor">@android:color/transparent</item> <!-- 状态栏透明 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item> <!-- activity窗口切换效果 -->
    </style>
    <declare-styleable name="CheckLineView" ns1:ignore="ResourceName">
        <!--    左侧check圈的半径-->
        <attr format="dimension" name="left_radius"/>
        <!--    选中之后的颜色-->
        <attr format="color" name="checked_line_color"/>
        <!--    未选中时的颜色-->
        <attr format="color" name="unchecked_line_color"/>
        <!--    是否已经check-->
        <attr format="boolean" name="is_checked"/>
        <!--    未check的圈圈的宽度-->
        <attr format="dimension" name="uncheck_line_width"/>
        <!--    check的圈圈的宽度-->
        <attr format="dimension" name="checked_line_width"/>
        <!--    是否绘制划线-->
        <attr format="boolean" name="with_check_line"/>
        <!--    起始角度-->
        <attr format="float" name="start_angle"/>
    </declare-styleable>
